          "_params": {
            "paddingTop": "5",
            "paddingBottom": "5",
            "parallax": true,
            "bgImageRadio": true,
            "bgImage": "@PROJECT_PATH@/assets/images/full-unsplash-photo-1485282087984-76b9f029bb05-2000x1333.jpg",
            "bgColor": "#F2F2F2",
            "bgColorRadio": false,
